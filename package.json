{
  "name": "angular2-universal-wordpress",
  "version": "0.0.1",
  "description": "Angular 2 Universal with WordPress REST API",
  "repository": {
    "type": "git",
    "url": "https://github.com/jussikinnula/angular2-universal-wordpress.git"
  },
  "scripts": {
    "prebuild:prod": "npm run clean",
    "build:prod": "webpack --config config/webpack.production.ts --progress",
    "prebuild:dev": "npm run clean",
    "build:dev": "webpack --config config/webpack.development.ts --progress",
    "clean": "rimraf target",
    "predev": "npm run build:dev",
    "dev": "concurrently 'npm run server:dev' 'npm run watch'",
    "postinstall": "npm run build",
    "server:prod": "node target/server/index.js",
    "server:dev": "nodemon target/server/index.js",
    "server": "npm run server:prod",
    "start": "npm run server",
    "watch": "webpack --config config/webpack.development.ts --watch"
  },
  "license": "MIT",
  "contributors": [
    "Jussi Kinnula <jussi.kinnula@frantic.com>"
  ],
  "dependencies": {
    "@angular/common": "~2.2.3",
    "@angular/compiler": "~2.2.3",
    "@angular/compiler-cli": "~2.2.3",
    "@angular/core": "~2.2.3",
    "@angular/forms": "~2.2.3",
    "@angular/http": "~2.2.3",
    "@angular/platform-browser": "~2.2.3",
    "@angular/platform-browser-dynamic": "~2.2.3",
    "@angular/platform-server": "~2.2.3",
    "@angular/router": "~3.2.3",
    "@angular/upgrade": "~2.2.3",
    "@ngtools/webpack": "~1.1.7",
    "@types/body-parser": "0.0.33",
    "@types/compression": "0.0.33",
    "@types/cookie-parser": "~1.3.30",
    "@types/express": "~4.0.34",
    "@types/express-serve-static-core": "~4.0.39",
    "@types/hammerjs": "~2.0.33",
    "@types/mime": "0.0.29",
    "@types/node": "~6.0.51",
    "@types/request": "0.0.34",
    "@types/serve-static": "~1.7.31",
    "@types/webfontloader": "~1.6.27",
    "angular2-express-engine": "~2.1.0-rc.1",
    "angular2-platform-node": "~2.1.0-rc.1",
    "angular2-template-loader": "~0.4.0",
    "angular2-universal": "~2.1.0-rc.1",
    "angular2-universal-polyfills": "~2.1.0-rc.1",
