{
  "name": "angular2-universal-wordpress",
  "version": "0.0.1",
  "description": "Angular 2 Universal with WordPress REST API",
  "repository": {
    "type": "git",
    "url": "https://github.com/jussikinnula/angular2-universal-wordpress.git"
  },
  "scripts": {
    "prebuild:prod": "npm run clean",
    "build:prod": "webpack --config config/webpack.production.ts --progress",
    "prebuild:dev": "npm run clean",
    "build:dev": "webpack --config config/webpack.development.ts --progress",
    "build": "npm run build:prod",
    "clean": "rimraf target",
    "predev:frontend": "npm run clean",
    "dev:frontend": "npm run server:dev:frontend",
    "predev:backend": "npm run clean && npm run build:dev",
    "dev:backend": "concurrently \"npm run server:dev:backend\" \"npm run watch\"",
    "dev": "npm run dev:frontend",
    "postinstall": "npm run build",
    "server:prod": "node target/server/index.js",
    "server:dev:frontend": "webpack-dev-server --config config/webpack.devserver.ts",
    "server:dev:backend": "nodemon target/server/index.js",
    "server:dev": "npm run server:dev:frontend",
    "server": "npm run server:prod",
    "start": "npm run server",
    "watch": "webpack --config config/webpack.development.ts --watch"
  },
  "license": "MIT",
  "contributors": [
    "Jussi Kinnula <jussi.kinnula@frantic.com>"
  ],
  "dependencies": {
    "@angular/common": "2.2.3",
    "@angular/compiler": "2.2.3",
    "@angular/core": "2.2.3",
    "@angular/forms": "2.2.3",
    "@angular/http": "2.2.3",
    "@angular/platform-browser": "2.2.3",
    "@angular/platform-browser-dynamic": "2.2.3",
    "@angular/platform-server": "2.2.3",
    "@angular/router": "3.2.3",
    "@angular/upgrade": "2.2.3",
    "@types/body-parser": "0.0.34",
    "@types/cookie-parser": "~1.3.30",
    "@types/express": "~4.0.35",
    "@types/express-serve-static-core": "~4.0.40",
    "@types/hammerjs": "~2.0.34",
    "@types/mime": "0.0.29",
    "@types/node": "~7.0.5",
    "@types/request": "0.0.41",
    "@types/serve-static": "~1.7.31",
    "@types/webfontloader": "~1.6.28",
    "angular2-express-engine": "2.1.0-rc.1",
    "angular2-platform-node": "2.1.0-rc.1",
    "angular2-template-loader": "0.6.2",
    "angular2-universal": "2.1.0-rc.1",
    "angular2-universal-polyfills": "2.1.0-rc.1",
    "autoprefixer": "~6.7.6",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "copy-webpack-plugin": "~4.0.1",
    "express": "~4.15.2",
    "extract-text-webpack-plugin": "~2.1.0",
    "favicons-webpack-plugin": "0.0.7",
    "html-webpack-plugin": "~2.28.0",
    "imports-loader": "~0.7.1",
    "js.clone": "0.0.3",
    "json-loader": "~0.5.4",
    "methods": "~1.1.2",
    "moment": "~2.17.1",
    "postcss-loader": "~1.3.3",
    "preboot": "~4.5.2",
    "pug-html-loader": "~1.1.1",
    "raw-loader": "~0.5.1",
    "reflect-metadata": "0.1.10",
    "request": "~2.80.0",
    "rimraf": "~2.6.1",
    "rxjs": "5.0.0-beta.12",
    "string-replace-loader": "~1.0.5",
    "stylus": "~0.54.5",
    "stylus-loader": "~3.0.1",
    "ts-helpers": "~1.1.2",
    "ts-loader": "~2.0.1",
    "ts-node": "~2.1.0",
    "typescript": "2.2.1",
    "webfontloader": "~1.6.27",
    "webpack": "~2.2.1",
    "webpack-bundle-analyzer": "2.3.0",
    "webpack-merge": "~4.0.0",
    "zone.js": "0.6.21"
  },
  "devDependencies": {
    "concurrently": "~3.4.0",
    "nodemon": "~1.11.0",
    "webpack-dev-server": "~2.4.1"
  }
}
